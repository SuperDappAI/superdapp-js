{
  "name": "@superdapp/agents",
  "version": "1.0.0",
  "description": "SuperDapp AI Agents SDK and CLI for Node.js/TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "superagent": "dist/cli/index.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "dev": "tsx watch src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "cli": "tsx src/cli/index.ts",
    "deploy:cloudflare": "wrangler deploy --config deploy/cloudflare/wrangler.toml",
    "deploy:cloudflare:staging": "wrangler deploy --env staging --config deploy/cloudflare/wrangler.toml",
    "deploy:cloudflare:production": "wrangler deploy --env production --config deploy/cloudflare/wrangler.toml",
    "deploy:aws": "sam build && sam deploy --config-file deploy/aws/template.yaml",
    "deploy:vercel": "vercel --cwd . --config deploy/vercel/vercel.json",
    "deploy:vercel:production": "vercel --prod --cwd . --config deploy/vercel/vercel.json"
  },
  "keywords": [
    "superdapp",
    "ai",
    "agents",
    "sdk",
    "cli",
    "typescript",
    "bot",
    "chat"
  ],
  "author": "SuperDapp Team",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@types/sharp": "^0.31.1",
    "aws-amplify": "^6.15.0",
    "axios": "^1.7.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "file-type": "^21.0.0",
    "form-data": "^4.0.0",
    "inquirer": "^9.2.19",
    "node-schedule": "^2.1.1",
    "ora": "^8.0.1",
    "sharp": "^0.34.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/node-fetch": "^2.6.12",
    "@types/node-schedule": "^2.1.7",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperDapp/superdapp-js.git"
  },
  "bugs": {
    "url": "https://github.com/SuperDapp/superdapp-js/issues"
  },
  "homepage": "https://github.com/SuperDapp/superdapp-js#readme"
}
